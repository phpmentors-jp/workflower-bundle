{
    "name": "phpmentors/workflower-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for Workflower",
    "keywords": ["flow", "workflow", "process", "bpm", "bpmn", "symfony"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "KUBO Atsuhiro",
            "email": "kubo@iteman.jp"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "phpmentors/workflower": "~1.2@dev",
        "symfony/config": "~2.7",
        "symfony/dependency-injection": "~2.7",
        "symfony/finder": "~2.7",
        "symfony/http-kernel": "~2.7",
        "symfony/security-core": "~2.7",
        "symfony/security-bundle": "~2.7"
    },
    "suggest": {
        "doctrine/orm": "provides transparent serialization/deserialization support for entities with Doctrine ORM",
        "symfony/doctrine-bridge": ">=2.7.0 provides transparent serialization/deserialization support for entities with Doctrine ORM"
    },
    "autoload": {
        "psr-4": {
            "PHPMentors\\WorkflowerBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
